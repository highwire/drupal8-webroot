{
    "name": "highwire/drupal-psr-16",
    "description": "Convert Drupal 8 Cache objects to PSR-16 compliant cache objects",
    "type": "library",
    "version": "1.0.0", 
    "license": "GPLv3",
    "keywords": [
        "drupal",
        "cache",
        "psr-16"
    ],
    "authors": [
        {
            "name": "phayes",
            "email": "patrick.d.hayes@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "HighWire\\DrupalPSR16\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8.28 <5",
        "drupal/core": "~8.3"
    },
    "require": {
        "psr/simple-cache": "^1.0"
    }
}
